on pipes do
 (),extract value where (each i in key 4 take i end)="pipe" from card.widgets
end

on rin p a b do
 ((p>a)~(1,1))&((p<b)~(1,1))
end

on view do
 if 0=(4%sys.frame)
  dy.value:dy.value+1
 end
  
 # pipes
 each i in pipes[]
  if (i.pos[0]+97)=bird.pos[0]
   score.value:score.value+1
   sdisp.text:"Score: %i" format (score.value)
   death.widgets.score.text: sdisp.text
  end
 end
 each i in pipes[]
  i.pos:i.pos+(-1,0)
  if i.pos[0] < (-97)
   card.remove[i]
  end
 end
 bird.pos:bird.pos+(0,dy.value)
 if 0=(300%sys.frame)
  i1:image[pipe]
  i2:image[pipe]
  i2.transform["vert"]
  pn:"pipe%i" format (sys.frame)
  p:card.add["canvas" pn]
  p.size:i1.size[0],(i1.size[1]*3)
  p.pos:510,(-random[160])
  p.paste[i1 (0,0) 0]
  p.paste[i2 (0,i1.size[1]+100) 0]
  p.border:0
  p.index:0
 end
 f:0
 each i in pipes[]
  p:i.pos
  p1:i.pos+(97,247)
  p2:i.pos+(0,347)
  p21:i.pos+(97,741)
  b:each i in ((list 0,0),(list 36,0),(list 0,24),(list 36,24)) 
   f:f|rin[(bird.pos)+i p p1]
   f:f|rin[(bird.pos)+i p2 p21]
  end
 end
 if (bird.pos[1]>320)|(bird.pos[1]<0)|f
  dy.value:0
  score.value:0
  sdisp.text:"Score: 0"
  bird.pos:113,150
  each i in pipes[]
   card.remove[i]
  end
  go["Next" "SlideLeft"]
 else
  go[card]
 end
end

